<routes xmlns="http://camel.apache.org/schema/spring"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://camel.apache.org/schema/spring
			https://camel.apache.org/schema/spring/camel-spring-4.2.0.xsd">
    <route id="binanceFutures">
        <from uri="seda:signalReceived"/>
        <setHeader name="BalancePercentage">
        	<constant>10</constant>
       	</setHeader>
        <to uri="log:messageBus?level=DEBUG"/>
        <bean ref="binanceFutures" method="processSignal"/>
    </route>
</routes>